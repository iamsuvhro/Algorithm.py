   ],
   "source": [
    "def linear_search(data_list,data):\n",
    "    index = len(data_list)\n",
    "    for i in range(0,index):\n",
    "        if data==data_list[i]:\n",
    "            print(\"Found\",i)\n",
    "            break\n",
    "        else:\n",
    "            print(\"Not in list\")     \n",
    "\n",
    "print(linear_search([5,2,3,5,8], 3))"
   ]
